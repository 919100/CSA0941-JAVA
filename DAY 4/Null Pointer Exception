class Ex
{
public void display()
{
 try
{
  String s=null;
  System.out.println("string length is"+ s.length());
}
catch(NullPointerException e)
{
 System.out.println("me raised exception");
}
 System.out.println("program ends");
}
}
public class Ad
{
public static void main(String a[])
{
 Ex obj=new Ex();
 obj.display();
}
}
